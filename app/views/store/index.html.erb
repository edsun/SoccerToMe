<div id="recent">
  <h3>
  Recent Items
  </h3>
  <% @products.each do |product| %>
    <%= link_to(image_tag(product.image_url, :height => '250px'), store_path(product.id)) %>
  <% end %>
</div>

<div id="sale">
  <h3>
    On Sale Items
  </h3>
  <p>On Sale Items go here</p>
</div>

<div id="brand">
  <h3>
    Shop by Brand:
  </h3>
  <p>Brand logos go here</p>
</div>


